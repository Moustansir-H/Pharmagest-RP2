<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="538.0"
            prefWidth="824.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="mcci.businessschool.bts.sio.slam.pharmagest.approvisionnement.controleur.ApprovisionnementControleur">

    <top>
        <AnchorPane prefHeight="73.0" prefWidth="824.0" style="-fx-background-color: green;"
                    BorderPane.alignment="CENTER">
            <children>
                <Label layoutX="256.0" layoutY="20.0" prefHeight="33.0" prefWidth="313.0"
                       text="GESTION APPROVISIONNEMENT">
                    <font>
                        <Font name="Berlin Sans FB" size="19.0"/>
                    </font>
                </Label>
            </children>
        </AnchorPane>
    </top>

    <center>
        <AnchorPane prefHeight="423.0" prefWidth="787.0" style="-fx-background-color: #9ed9a1;"
                    BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="receptionCommandeButton" layoutX="160.0" layoutY="180.0" mnemonicParsing="false"
                        prefHeight="60.0" prefWidth="200.0" style="-fx-background-color: green; -fx-text-fill: white;"
                        text="Réception Commande"/>

                <Button fx:id="generationCommandeButton" layoutX="460.0" layoutY="180.0" mnemonicParsing="false"
                        prefHeight="60.0" prefWidth="200.0" style="-fx-background-color: green; -fx-text-fill: white;"
                        text="Génération Commande" onAction="#ouvrirGenerationCommande"/>

                <Button fx:id="retourDashboard" layoutX="320.0" layoutY="300.0" mnemonicParsing="false"
                        prefHeight="50.0" prefWidth="180.0" style="-fx-background-color: red; -fx-text-fill: white;"
                        text="Retour" onAction="#retourDashboardOnAction"/>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
